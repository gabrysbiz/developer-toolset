<?xml version='1.0' encoding='UTF-8'?>
<profile>
    <name>gabrys.biz</name>
    <language>jproperties</language>
    <rules>
        <rule>
            <repositoryKey>common-jproperties</repositoryKey>
            <key>DuplicatedBlocks</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>common-jproperties</repositoryKey>
            <key>FailedUnitTests</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>common-jproperties</repositoryKey>
            <key>InsufficientBranchCoverage</key>
            <priority>MAJOR</priority>
            <parameters>
                <parameter>
                    <key>minimumBranchCoverageRatio</key>
                    <value>65</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>common-jproperties</repositoryKey>
            <key>InsufficientCommentDensity</key>
            <priority>MAJOR</priority>
            <parameters>
                <parameter>
                    <key>minimumCommentDensity</key>
                    <value>25</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>common-jproperties</repositoryKey>
            <key>InsufficientLineCoverage</key>
            <priority>MAJOR</priority>
            <parameters>
                <parameter>
                    <key>minimumLineCoverageRatio</key>
                    <value>65</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>common-jproperties</repositoryKey>
            <key>SkippedUnitTests</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>S1134</key>
            <priority>INFO</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>S1135</key>
            <priority>INFO</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>S1578</key>
            <priority>MINOR</priority>
            <parameters>
                <parameter>
                    <key>format</key>
                    <value>^[A-Za-z][-_A-Za-z0-9]*\.properties$</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>S2068</key>
            <priority>CRITICAL</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>S2260</key>
            <priority>CRITICAL</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>bom-utf8-files</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>comment-convention</key>
            <priority>MINOR</priority>
            <parameters>
                <parameter>
                    <key>startingCommentToken</key>
                    <value>#</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>commented-out-code</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>duplicated-keys</key>
            <priority>CRITICAL</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>duplicated-keys-across-files</key>
            <priority>CRITICAL</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>duplicated-values</key>
            <priority>CRITICAL</priority>
            <parameters>
                <parameter>
                    <key>valuesToIgnore</key>
                    <value>true,false,yes,no,0,1,-1</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>empty-element</key>
            <priority>CRITICAL</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>empty-line-end-of-file</key>
            <priority>MINOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>indentation</key>
            <priority>MINOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>key-naming-convention</key>
            <priority>MINOR</priority>
            <parameters>
                <parameter>
                    <key>Format</key>
                    <value>^[A-Za-z][.A-Za-z0-9]*$</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>line-length</key>
            <priority>MINOR</priority>
            <parameters>
                <parameter>
                    <key>maximumLineLength</key>
                    <value>120</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>maximum-number-keys</key>
            <priority>MAJOR</priority>
            <parameters>
                <parameter>
                    <key>numberKeys</key>
                    <value>200</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>separator-convention</key>
            <priority>MINOR</priority>
            <parameters>
                <parameter>
                    <key>Separator</key>
                    <value>=</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>jproperties</repositoryKey>
            <key>tab-character</key>
            <priority>MINOR</priority>
            <parameters />
        </rule>
    </rules>
</profile>