<?xml version='1.0' encoding='UTF-8'?>
<profile>
    <name>gabrys.biz</name>
    <language>xml</language>
    <rules>
        <rule>
            <repositoryKey>common-xml</repositoryKey>
            <key>DuplicatedBlocks</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>common-xml</repositoryKey>
            <key>FailedUnitTests</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>common-xml</repositoryKey>
            <key>InsufficientBranchCoverage</key>
            <priority>MAJOR</priority>
            <parameters>
                <parameter>
                    <key>minimumBranchCoverageRatio</key>
                    <value>65</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>common-xml</repositoryKey>
            <key>InsufficientLineCoverage</key>
            <priority>MAJOR</priority>
            <parameters>
                <parameter>
                    <key>minimumLineCoverageRatio</key>
                    <value>65</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>common-xml</repositoryKey>
            <key>SkippedUnitTests</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>IllegalTabCheck</key>
            <priority>MINOR</priority>
            <parameters>
                <parameter>
                    <key>markAll</key>
                    <value>true</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>IndentCheck</key>
            <priority>MINOR</priority>
            <parameters>
                <parameter>
                    <key>indentSize</key>
                    <value>4</value>
                </parameter>
                <parameter>
                    <key>tabSize</key>
                    <value>4</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>NewlineCheck</key>
            <priority>MINOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>S103</key>
            <priority>MAJOR</priority>
            <parameters>
                <parameter>
                    <key>maximumLineLength</key>
                    <value>120</value>
                </parameter>
            </parameters>
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>S1134</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>S1135</key>
            <priority>INFO</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>S125</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>S1778</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
        <rule>
            <repositoryKey>xml</repositoryKey>
            <key>S2260</key>
            <priority>MAJOR</priority>
            <parameters />
        </rule>
    </rules>
</profile>